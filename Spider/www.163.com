(function($){
	initSidebar();
	function initSidebar(){
        
        if(!!window.ActiveXObject && !window.XMLHttpRequest) return;
               
    	var timer,sidebar = $('.ns-sidebar'),sideTolid = $('.ns-side-tolid', sidebar),sideQrcode = $('.ns-side-qrcode', sidebar),popupQrcode = $('.ns-popup-qrcode', sidebar);
        sideTolid.addEvent("click",function() {
            var top = document.body.scrollTop || document.documentElement.scrollTop;
            var left = document.body.scrollLeft || document.documentElement.scrollLeft;
            if(top > 0) {
                top -= (top/3) + 10;
                window.scrollTo(left, top);
                setTimeout(arguments.callee, 10);
            } else {
                window.scrollTo(left, 0);
                sidebar.addCss("none");
            }
        });  
        sideQrcode.addEvent('mouseover', function() {
            popupQrcode[0].style.display = 'block';
            clearTimeout(timer);
        });
        sideQrcode.addEvent('mouseout', function() {
            timer = setTimeout(function() {
                popupQrcode[0].style.display = 'none';
            }, 100);
        });
        popupQrcode.addEvent('mouseover', function() {
            clearTimeout(timer);
        });
        
        popupQrcode.addEvent('mouseout', function(e) {
            timer = setTimeout(function() {
                popupQrcode[0].style.display = 'none';
            }, 100);
        });
            
        $(window).addEvent("scroll", function() {
            var top = document.body.scrollTop || document.documentElement.scrollTop;
            if(top > 600) {
                sidebar[0].style.display = 'block';
            } else {
                sidebar[0].style.display = 'none';
            }
        });    
    }
