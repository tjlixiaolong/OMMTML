		        NTES.ajax.importJs('http://img1.cache.netease.com/f2e/finance/preterite/js_2010/stocksearch_suggest-1.1.js', function() {
		            var stockSuggest = new ntesStockSuggest({
		                inputid : "socode",
		                max : 10,
